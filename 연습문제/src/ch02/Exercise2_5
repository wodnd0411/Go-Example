Exercise2_5

System.out.println("1" + "2"); // "12"
System.out.println(true + ""); // "true"
System.out.println('A' + 'B'); // 'A' + 'B' = 65 + 66 = 131
System.out.println('1' + 2); // '1' + 2 = 49 + 2 = 51
System.out.println('1' + '2'); // '1' + '2' = 49 + 50 = 99
System.out.println('J' + "ava"); // 'J' + "ava" = "J" + "ava" = "Java"
System.out.println(true + null); // ¿¡·¯